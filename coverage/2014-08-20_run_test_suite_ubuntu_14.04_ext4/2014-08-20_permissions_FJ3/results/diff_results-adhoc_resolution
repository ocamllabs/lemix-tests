        Tau
        { st_dev=2052; st_ino=4067085; st_kind=S_IFREG; st_perm=0o644; st_nlink=1; st_uid=0; st_gid=0; st_rdev=0; st_size=0; }

    38: stat /d1/d2/file2.txt
