        # tesing pwrite and write


        ##############################################
        # initialization
        ##############################################

        # empty and non-empty directories to write to
     9: mkdir /empty_dir 0o777
        RV_none
    10: mkdir /non_empty_dir 0o777
        RV_none

    12: chdir /non_empty_dir
        RV_none


